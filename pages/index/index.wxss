page {
    background-color: #fafafa;
}

.logo-box {
    display: flex;
    justify-content: center;
    padding-top: 100rpx;
}

.logo {
    width: 150rpx;
    height: 150rpx;
    border-radius: 100%;
}

.logs {
    padding: 20rpx 30rpx;
}

.username {
    display: flex;
    padding: 20rpx 0;
    padding-top: 100rpx;
    border-bottom: 1px solid #9884bd;
}

.username input {
    width: 550rpx;
}

.password {
    display: flex;
    padding: 20rpx 0;
    padding-top: 50rpx;
    border-bottom: 1px solid #d0d0d1;
}

.password input {
    width: 550rpx;
}

.password label {
    color: #c0c0c0;
}

.subm {
    font-size: 35rpx;
    margin-top: 100rpx;
    border: 0;
    background-color: #564e61;
    color: #fff;
}

.img_foter {
    position: absolute;
    width: 100%;
    height: 400rpx;
    left: 0;
    bottom: 0;
    overflow: hidden;
    text-align: center;
}

.img_foter image {
    width: 100%;
    height: 100%;
    border: 0;
}

.shuju {
    position: absolute;
    left: 0;
    bottom: 20rpx;
    width: 100%;
    height: 40rpx;
    text-align: center;
    font-size: 24rpx;
}

.findIcon image {
    position: fixed;
    left: 308rpx;
    bottom: 20rpx;
    border-radius: 50%;
    width: 130rpx;
    height: 130rpx;
}